{
  "expo": {
    "name": "Smart Money",
    "slug": "smart-money",
    "version": "1.3.5",
    "date": "June 27 2025",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon.png",
    "scheme": "smartmoney",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Aplikasi memerlukan akses untuk menyimpan backup"
      },
      "bundleIdentifier": "com.koucheng.smartmoney"
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "POST_NOTIFICATIONS",
        "CAMERA",
        "RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#22a7a7"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.koucheng.smartmoney"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-document-picker",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon-light.png",
          "imageWidth": 180,
          "resizeMode": "cover",
          "backgroundColor": "#00897B"
        }
      ],[
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5a0e1649-4270-4f53-bf16-89334f8fa9fa"
      }
    },
    "owner": "koucheng"
  }
}
